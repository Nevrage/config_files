# xscreensaver.Dialog.internalBorderWidth:0
# xscreensaver.Dialog.borderWidth:        0
# xscreensaver.Dialog.shadowThickness:    0
# !timeout bar (background is actually determined by Dialog.text.background)
# xscreensaver.passwd.thermometer.foreground:  #A9B7C4
# xscreensaver.passwd.thermometer.background:  #202020
# xscreensaver.passwd.thermometer.width:       8
# !datestamp format--see the strftime(3) manual page for details
# xscreensaver.dateFormat:    %I:%M%P %a %b %d, %Y
 urxvt*transparent: true
# urxvt*depth: 32
# urxvt*background: rgba:0000/0000/0200/c800
 URxvt*scrollBar: false
# URxvt*urgentOnBell: false
urxvt*shading: 30
urxvt.font: xft:Ubuntu Nerd Font:medium:size=15
# #URxvt.font: xft:Nakula:size=15
# Xft.autohint:   0
# Xft.dpi:        96
# Xft.antialias:  true
 Xft.rgba:       none
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.lcdfilter: lcddefault
URxvt.letterSpace: -1
URxvt*termName:  rxvt-256color
URxvt*iso14755: False
